{
  "name": "vue-weather-wc",
  "description": "A web component that provides simple weather data",
  "version": "1.0.0",
  "private": false,
  "main": "dist/weather.umd.js",
  "files": ["dist/weather.umd"],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-wc": "vue-cli-service build --target wc --name weather --inline-vue src/weather.js",
    "build-lib": "vue-cli-service build --target lib --name weather src/weather.js"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
